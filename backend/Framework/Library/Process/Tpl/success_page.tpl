<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>{$data.title}</title>
    <style>
        body {
            text-align: center;
            margin-top: 10%;
        }

        h1 {
            color: #666;
            font-size: 36px;
        }

        p {
            color: #999
        }

        .btn {
            width: 25%;
            height: 60px;
            background: #028bfa;
            line-height: 60px;
            cursor: pointer;
            color: #fff;
            font-size: 18px;
            border-radius: 4px;
            margin: 0 auto;
        }
    </style>
</head>

<body>
<img src="data:image/jpg;base64," />
<h1>{$data.message}</h1>
<p>{$data.describe}</p>
<div class="btn" onclick="window.history.back(-1);">返回上一页</div>
</body>
<script type="text/javascript">
    var i = {$data.second} -1;
    var url = "{$data.url}";
    var intervalid;
    intervalid = setInterval("fun()", 1000);

    function fun() {
        if (i == 0) {
            if (url == '') {
                window.history.back(-1);
            } else {
                location.href = url;
            }
            clearInterval(intervalid);
        }
        i--;
    }
</script>
</html>